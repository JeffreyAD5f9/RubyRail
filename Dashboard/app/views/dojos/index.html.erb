
<a href="/dojos/new">Create New</a>

    <table>
      <tr>
        <th>Branch</th>
        <th>Street</th>
        <th>City</th>
        <th>State</th>
        <th>Actions</th>
      </tr>
      <% @dojos.each do |dojo|  %>
        <tr>
          <td><%= dojo.branch %></td>
          <td><%= dojo.street %></td>
          <td><%= dojo.city %></td>
          <td><%= dojo.state %></td>
          <td><a href = "/dojos/<%= dojo.id %>/students">Show</a></td>
          <td><a href = "/dojos/<%= dojo.id %>/edit">Edit</a></td>
          <td><a href = "/dojos/<%= dojo.id %>" data-method="delete">Delete</a></td>
        </tr>
      <% end %>
    </table>
