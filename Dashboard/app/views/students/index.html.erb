
<a href="/dojos/<%= @dojo.id %>/students/new">Create New</a>


<h3><%= @dojo.branch %> Dojo </h3>

    <table>
      <tr>
        <th>Branch</th>
        <th>Street</th>
        <th>City</th>
        <th>State</th>
        <th>Actions</th>
      </tr>
        <tr>
          <td><%= @dojo.branch %></td>
          <td><%= @dojo.street %></td>
          <td><%= @dojo.city %></td>
          <td><%= @dojo.state %></td>
          <td><a href = "/dojos/<%= @dojo.id %>/edit">Edit</a></td>

        </tr>
    </table>

    <h3> Students </h3>

    <table>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Actions</th>
      </tr>
      <% @students.each do |student|  %>
        <tr>
          <td><%= student.first_name %></td>
          <td><%= student.last_name %></td>
          <td><%= student.email %></td>
          <td><a href = "/dojos/<%= @dojo.id %>/students/<%= student.id %>">Show</a></td>
          <td><a href = "/dojos/<%= @dojo.id %>/students/<%= student.id %>/edit">Edit</a></td>
          <td><a href = "/dojos/<%= @dojo.id %>/students/<%= student.id %>" data-method="delete">Delete</a></td>
        </tr>
      <% end %>
    </table>
